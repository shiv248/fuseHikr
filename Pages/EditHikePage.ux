<Page ux:Class ="EditHikePage">
	<JavaScript File="EditHikePage.js"/>
	<ScrollView>
		<StackPanel>
			<Text>Name:</Text>
			<TextBox Value="{name}"/>
			<Text>Location:</Text>
			<TextBox Value="{location}"/>
			<Text>Distance:</Text>
			<TextBox Value="{distance}" InputHint="Decimal"/>
			<Text>Rating:</Text>
			<TextBox Value="{rating}" InputHint="Integer"/>
			<Text>Comments:</Text>
			<TextView Value="{comments}" TextWrapping="Wrap"/>
		</StackPanel>
	</ScrollView>
</Page>
